{
  "name": "ai-copilot-transaction-builder",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p .",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mysten/sui": "^1.45.2",
    "pg": "^8.16.0",
    "express": "^5.2.1",
    "pino": "^10.1.0",
    "jose": "^5.2.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.2",
    "@vitest/coverage-v8": "^1.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.2.0"
  }
}
